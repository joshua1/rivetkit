{
  "name": "@rivetkit/svelte",
  "version": "0.9.0-rc.1",
  "license": "Apache-2.0",
  "keywords": ["rivetkit", "svelte", "framework", "actors", "stateful", "serverless"],
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mod.d.ts",
        "default": "./dist/mod.js"
      },
      "require": {
        "types": "./dist/mod.d.cts",
        "default": "./dist/mod.cjs"
      }
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "check-types": "tsc --noEmit"
  },
  "peerDependencies": {
    "@rivetkit/core": "*",
    "svelte": "^5.0.0"
  },
  "dependencies": {
    "@rivetkit/framework-base": "workspace:*"
  },
  "devDependencies": {
    "@rivetkit/core": "workspace:*",
    "svelte": "^5.0.0",
    "typescript": "^5.5.2",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.1.1"
  },
  "stableVersion": "0.8.0"
}
